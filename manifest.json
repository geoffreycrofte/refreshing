{
    "name": "Refrecsser",
    "description": "Refrecsser",
    "version": ".26",
    "permissions": [
        "activeTab",
        "storage",
        "webNavigation",
        "*://*/*"
    ],
    "background": {
        "scripts": ["background.js", "jquery.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["https://vserver-eddie/*", "http://vserver-eddie/*"],
            "js": ["setData.js","jquery.js","main.js"],
            "css": [ "main.css"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "web_accessible_resources": [
        "icon16.png",
        "icon48.png"
    ],
    "browser_action": {
        "default_title": "Click for options",
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
